{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*\\.html|.*\\.css|.*\\.js|.*\\.jpg|.*\\.png|.*\\.svg)",
      "dest": "static/$1"
    },
    {
      "src": "/",
      "dest": "static/index.html"
    }
  ]
}
