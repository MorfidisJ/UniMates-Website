{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(about|how-it-works|quiz|Privacy-policy|customer-feedback|matching-survey)",
      "dest": "static/pages/$1.html"
    },
    {
      "src": "/",
      "dest": "static/pages/index.html"
    },
    {
      "src": "/styles/(.*)",
      "dest": "static/styles/$1"
    },
    {
      "src": "/scripts/(.*)",
      "dest": "static/scripts/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "static/assets/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "static/pages/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "static/scripts/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "static/styles/$1"
    }
  ]
}

